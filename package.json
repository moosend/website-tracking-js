{
  "name": "mootracker",
  "version": "0.10.0",
  "description": "Tracking API",
  "main": "lib/main.js",
  "scripts": {
    "start": "webpack-dev-server --inline",
    "test": "ts-node node_modules/tape/bin/tape test/*.test.ts | tap-spec",
    "coverage": "nyc ts-node node_modules/tape/bin/tape test/*.test.ts | tap-spec",
    "build-watch-tests": "tsc --noImplicitAny --outDir compiled-tests/ --module commonjs test/*.test.ts --watch -t es5",
    "demo": "cross-env EXPRESS_PORT=9911 node demo/server/main.js",
    "clean-dist": "rm -r dist/**",
    "build": "webpack",
    "build-prod": "webpack --config webpack.config.prod.js",
    "build-min": "webpack --config webpack.config.min.js",
    "build-demo": "webpack --config webpack.config.demo.js",
    "bundle-stage": "NODE_ENV=stage webpack",
    "bundle-loader": "uglifyjs src/loader.js -m -c > dist/loader.js",
    "compile": "tsc"
  },
  "author": "igor ovsiannikov <garek@mail.ru>",
  "license": "UNLICENSED",
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "awesome-typescript-loader": "^3.2.1",
    "body-parser": "^1.17.2",
    "debug": "^2.6.8",
    "express": "^4.15.3",
    "jest": "^20.0.4",
    "jsdom": "^11.1.0",
    "jsdom-global": "^3.0.2",
    "node-libs-browser": "^2.0.0",
    "nyc": "^11.0.3",
    "sinon": "^2.4.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.7.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.5.0",
    "typescript": "^2.4.2",
    "uglify-js": "^3.0.26",
    "vhost": "^3.0.0",
    "webpack": "^3.4.1",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-dev-server": "^2.6.1",
    "webpack-md5-hash": "0.0.5",
    "webpack-strip": "^0.1.0"
  },
  "dependencies": {
    "@types/es6-shim": "^0.31.34",
    "@types/node": "^8.0.17",
    "@types/sinon": "^2.3.3",
    "@types/tape": "^4.2.30",
    "@types/uuid": "^3.4.0",
    "cross-env": "^5.0.1",
    "es5-shim": "^4.5.9",
    "fingerprintjs2": "^1.5.1",
    "js-cookie": "^2.1.4",
    "json3": "^3.3.2",
    "lodash": "^4.16.6",
    "uuid": "^3.1.0"
  },
  "nyc": {
    "include": [
      "src/**/*/ts"
    ],
    "exclude": [],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node"
    ],
    "reporter": [
      "json",
      "html"
    ],
    "all": true
  }
}
